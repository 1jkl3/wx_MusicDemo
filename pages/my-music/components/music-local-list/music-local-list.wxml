<!--components/music_me/music-local-list/music-local-list.wxml-->
<view class="music-local-list">
  <block wx:for="{{musiclist}}" wx:key="index">
    <view class="music-list">
      <view class="music-icon">
        <text class="iconfont {{item.m_icon}}"></text>
      </view>
      <view class="music-title">
        <view class="music-listname">{{item.m_title}}</view>
        <view class="music-num">{{item.m_num}}</view>
      </view>
    </view>
  </block>
  <!-- <block wx:for="{{test}}" wx:key="index">
    <text>{{item}}</text>
  </block> -->
</view>